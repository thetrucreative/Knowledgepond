<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trucreative.knowledgepond">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name="com.trucreative.knowledgepond.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.trucreative.knowledgepond.registerActivity" />
        <activity android:name="com.trucreative.knowledgepond.simpleRules" />
        <activity android:name="com.trucreative.knowledgepond.MainActivity" />
        <activity android:name="com.trucreative.knowledgepond.highScores"></activity>
    </application>

</manifest>